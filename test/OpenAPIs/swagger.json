      "get": {
        "tags": [
          "Patient"
        ],
        "summary": "Gets the List of Patients",
        "operationId": "GetPatients",
        "responses": {
          "200": {
            "description": "Success"
          },
          "404": {
            "description": "Not Found",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          }
        }
      },
      "post": {
        "tags": [
          "Patient"
        ],
        "operationId": "PostPatient",
