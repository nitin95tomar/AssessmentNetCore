      },
      "post": {
        "tags": [
          "ProgressNote"
        ],
        "operationId": "PostProgressNotes",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/components/schemas/ProgressNote"
                }
              }
            },
            "text/json": {
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/components/schemas/ProgressNote"
                }
              }
            },
            "application/*+json": {
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/components/schemas/ProgressNote"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
